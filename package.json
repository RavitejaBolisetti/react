    "lint-staged": {
        "*.{js,jsx}": [
            "jest --bail --findRelatedTests",
            "git add"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
